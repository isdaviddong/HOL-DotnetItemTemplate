<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageType>Template</PackageType>
    <PackageId>Company.Namespace.ItemTempalteName</PackageId>
    <Title>XXXXXXXXXXXX  Item Template</Title>
    <Authors>作者名稱</Authors>
    <Description>Company.Namespace.ItemTempalteName</Description>
    <PackageTags>dotnet-new;templates;</PackageTags>

    <IncludeContentInPack>true</IncludeContentInPack>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <ContentTargetFolders>content</ContentTargetFolders>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>Company Name</Company>
    <Version>1.0.18</Version>
    <Product>Product Name</Product>
    <Copyright>Copyright</Copyright>
    <PackageReleaseNotes>改版資訊</PackageReleaseNotes>
    <AssemblyVersion>1.0.18</AssemblyVersion>
    <FileVersion>1.0.18</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="templates\**\*" Exclude="templates\**\bin\**;templates\**\obj\**" />
    <Compile Remove="**\*" />
  </ItemGroup>


</Project>
